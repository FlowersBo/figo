/**index.wxss**/
.container {
  background-color: #FFF;
  min-height: 100%
}
.header{
width:750rpx;
background: #00c482;
display: flex;
flex-direction: row;
justify-content: center;
  padding-top:40rpx;
  padding-bottom: 40rpx;
}
.header .search{
  width:650rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 5rpx;
  background:#FFF;
  display: flex;
  flex-direction: row;
  justify-content: center;

}
.header .search view{
    color:#a6a6a6;
    font-size: .7rem;
    align-self: center;

}
.header .search icon{

margin-right: 30rpx;
margin-top: 18rpx;

}


.swiper-container{
  width: 750rpx;
  position: relative;  
}
.swiper_box {
  width: 100%;
  height:362.5rpx;
}

swiper-item image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
  height:362.5rpx;
}
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 20rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 14rpx;  
  height: 14rpx;  
  background: #fff;  
  border-radius: 50%;  
  transition: all .6s;  
  opacity: .5;
}   
.swiper-container .dots .dot.active{  
  width: 14rpx;  
  opacity: 1;
}



.navlist{
 width:750rpx;
 display: flex;
 justify-content:space-around;
 flex-direction: row;
 padding-top:20rpx;
 flex-wrap: wrap;
}

.navlist .item{
    width:150rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 24rpx;
    padding-bottom: 30rpx;
    color: #656565;
}
.navlist .item image{
  width:120rpx;
  height: 120rpx;
}

.message{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: space-around;
}
.message-text{
  font-size: 24rpx;
  color: #dbdbdb;
  display: flex;
  align-items: center;
}
.message-text image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 5rpx;
}

/*流程*/
.serviceProcess{
  width: 96%;
  margin: auto;
  position: relative;
}
.serviceProcess image{
  width: 100%;
  height: 150rpx;
}
.serviceProcess .show{
  width: 120rpx; 
  height: 120rpx;
  background-color: pink;
  border-radius: 50%;
  position: absolute;
  right: 10rpx;
  top: 25rpx;
  opacity: 0;
}
/* 车友圈 */
.news{
  width: 96%;
  margin: auto;
  /* border-top: 2rpx solid #ddd; */
}
.newstitle{
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 36rpx;
}
.newstitle .viewmore{
  font-size: 24rpx;
  color: #dbdbdb;
}

.list{
  width: 100%;
  margin-top: 20rpx;
}
.list-text{
  font-size: 26rpx;
  color: #aaa;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.list-img{
  width: 100%;
  height: 336rpx;
  margin-top: 20rpx;
  position: relative;
}
.list-img image{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}
.list-img .time{
  font-size: 20rpx;
  color: white;
  position: absolute;
  bottom: 0;
  right: 16rpx;
}
